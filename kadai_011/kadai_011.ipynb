        "#配列array\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "#forを使う\n",
        "for arrays in array:\n",
        "    print(arrays)\n",
        "\n",
        "#whileを使う\n",
        "index = 0 #インデックス値の初期値\n",
        "\n",
        "while index < 9 : #インデックスが８以下の場合繰り返す\n",
        "    print(array[index])\n",
        "\n",
        "    index = index + 1\n",
        "\n"
      ],
